{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true
            }
        }
    },
    "list.source.files.defaultOptions": ["--number-select"],
    "coc.preferences.formatOnSaveFiletypes": [ "dart" ]
}
